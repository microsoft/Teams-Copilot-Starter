{
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "type": "AdaptiveCard",
    "version": "1.4",
    "body": [
        {
            "type": "Container",
            "items": [
                {
                    "type": "ColumnSet",
                    "columns": [
                        {
                            "type": "Column",
                            "width": "auto",
                            "items": [
                                {
                                    "type": "Image",
                                    "url": "data:image/png;base64,",
                                    "altText": "Teams Copilot Starter Banner",
                                    "horizontalAlignment": "Left"
                                }
                            ]
                        }
                    ]
                }
            ],
            "bleed": true,
            "verticalContentAlignment": "Center"
        },
        {
            "type": "TextBlock",
            "text": "${if(empty([entity.companyInfo.name]), 'N/A', entity.companyInfo.name)}",
            "weight": "Bolder",
            "size": "Large",
            "spacing": "None",
            "wrap": true,
            "style": "heading"
        },
        {
            "type": "TextBlock",
            "text": "Stock Market: ${if(empty([entity.companyInfo.ticker]), 'N/A', [entity.companyInfo.ticker])}",
            "spacing": "None",
            "size": "Small",
            "wrap": true,
            "isSubtle": true
        },
        {
            "type": "ColumnSet",
            "$data": "${entity}",
            "columns": [
                {
                    "type": "Column",
                    "width": "stretch",
                    "items": [
                        {
                            "type": "Container",
                            "items": [
                                {
                                    "type": "ColumnSet",
                                    "columns": [
                                        {
                                            "type": "Column",
                                            "width": "21px",
                                            "items": [
                                                {
                                                    "type": "Image",
                                                    "url": "data: image/png;base64,",
                                                    "width": "15px",
                                                    "height": "15px",
                                                    "horizontalAlignment": "Left"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "Column",
                                            "width": "auto",
                                            "spacing": "None",
                                            "items": [
                                                {
                                                    "type": "TextBlock",
                                                    "text": "Sanctions status: ",
                                                    "spacing": "None",
                                                    "wrap": true,
                                                    "weight": "Bolder"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "Column",
                                            "width": "stretch",
                                            "items": [
                                                {
                                                    "type": "TextBlock",
                                                    "text": "${if(empty([watchListStatus]), '', watchListStatus)}"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "spacing": "None",
                            "verticalContentAlignment": "Center"
                        },
                        {
                            "type": "Container",
                            "spacing": "None",
                            "verticalContentAlignment": "Center",
                            "items": [
                                {
                                    "type": "ColumnSet",
                                    "columns": [
                                        {
                                            "type": "Column",
                                            "width": "21px",
                                            "items": [
                                                {
                                                    "type": "Image",
                                                    "url": "data: image/png;base64,",
                                                    "width": "15px",
                                                    "height": "15px",
                                                    "horizontalAlignment": "Left"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "Column",
                                            "spacing": "None",
                                            "items": [
                                                {
                                                    "type": "TextBlock",
                                                    "text": "${if(empty([companyInfo.address.city_state]), '', companyInfo.address.city_state)}",
                                                    "spacing": "None",
                                                    "wrap": true
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "type": "Container",
                            "spacing": "None",
                            "verticalContentAlignment": "Center",
                            "items": [
                                {
                                    "type": "ColumnSet",
                                    "columns": [
                                        {
                                            "type": "Column",
                                            "width": "21px",
                                            "items": [
                                                {
                                                    "type": "Image",
                                                    "url": "data: image/png;base64,",
                                                    "width": "15px",
                                                    "height": "15px",
                                                    "horizontalAlignment": "Left"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "Column",
                                            "spacing": "None",
                                            "items": [
                                                {
                                                    "type": "TextBlock",
                                                    "text": "${if(empty([companyInfo.website]), '', companyInfo.website)}",
                                                    "spacing": "None",
                                                    "wrap": true,
                                                    "size": "Default",
                                                    "weight": "Bolder",
                                                    "color": "Accent"
                                                }
                                            ],
                                            "width": "stretch"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "Column",
                    "items": [
                        {
                            "type": "ColumnSet",
                            "columns": [
                                {
                                    "type": "Column",
                                    "width": "90px",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "Employees:",
                                            "weight": "Bolder",
                                            "spacing": "None",
                                            "style": "heading"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "Revenue:",
                                            "weight": "Bolder",
                                            "spacing": "None",
                                            "style": "heading"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "Industry:",
                                            "weight": "Bolder",
                                            "spacing": "None",
                                            "style": "heading",
                                            "wrap": true
                                        }
                                    ],
                                    "horizontalAlignment": "Left"
                                },
                                {
                                    "type": "Column",
                                    "width": "auto",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "${employees}",
                                            "spacing": "None"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "${if(empty([annualRevenue]), 'N/A', annualRevenue)}",
                                            "spacing": "None"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "${if(empty([industry]), 'N/A', industry)}",
                                            "spacing": "None"
                                        }
                                    ],
                                    "verticalContentAlignment": "Top",
                                    "horizontalAlignment": "Left"
                                }
                            ]
                        }
                    ],
                    "width": "stretch"
                }
            ]
        },
        {
            "type": "TextBlock",
            "text": "\n"
        },
        {
            "type": "TextBlock",
            "text": "ESG Score*",
            "weight": "Bolder",
            "size": "Large",
            "spacing": "None",
            "wrap": true,
            "style": "heading"
        },
        {
            "type": "TextBlock",
            "text": "NOTE: The ESG score shown here is for illustrative purposes only and does not represent actual performance.",
            "spacing": "None",
            "size": "Small",
            "wrap": true,
            "isSubtle": true
        },
        {
            "type": "ColumnSet",
            "columns": [
                {
                    "type": "Column",
                    "width": "stretch",
                    "items": [
                        {
                            "type": "ColumnSet",
                            "columns": [
                                {
                                    "type": "Column",
                                    "items": [
                                        {
                                            "type": "Image",
                                            "url": "data: image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' version='1.1' viewBox='0 0 200 100'><g transform='rotate(180 100 100)' fill='rgb(241, 242, 242)'><path d='M 0 100 A 1 1 0 0 0 200 100 L 190 100 A 1 1 0 0 1 10 100' /></g><g transform='rotate(${entity.esg.score * 1.8} 100 100)' fill='${if(entity.esg.rating == 'Weak performance', 'rgb(234, 65, 54)', if(entity.esg.rating == 'Limited performance', 'rgb(242, 167, 58)', 'rgb(124, 179, 66)'))}'><path d='M 0 100 A 1 1 0 0 0 200 100 L 190 100 A 1 1 0 0 1 10 100' /></g><g><text font-family='Segoe UI Semibold' font-size='28px' x='100' y='60' text-anchor='middle'>${entity.esg.score}</text><text font-family='Segoe UI Semibold' font-size='14px' x='100' y='80' text-anchor='middle'>${entity.esg.rating}</text><text font-family='Segoe UI' font-size='10px' x='100' y='100' text-anchor='middle'>${entity.esg.date}</text><text font-family='Segoe UI' font-size='10px' x='20' y='100' text-anchor='middle'>0</text><text font-size='10px' x='175' y='100' text-anchor='middle'>100</text></g></svg>",
                                            "width": "200px",
                                            "height": "100px",
                                            "horizontalAlignment": "Center"
                                        }
                                    ],
                                    "width": "stretch"
                                },
                                {
                                    "type": "Column",
                                    "items": [
                                        {
                                            "type": "Image",
                                            "url": "data: image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' version='1.1' viewBox='0 0 40 100'><line x1='20' y1='1' x2='20' y2='100' style='stroke:rgb(220,220,220);stroke-width:3'></line></svg>",
                                            "horizontalAlignment": "Center"
                                        }
                                    ],
                                    "width": "40px",
                                    "horizontalAlignment": "Center"
                                },
                                {
                                    "type": "Column",
                                    "items": [
                                        {
                                            "type": "Image",
                                            "url": "data: image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' version='1.1' viewBox='0 0 200 100'><g font-family='Segoe UI' font-size='12px'><text x='0' y='27' text-anchor='start'>Environmental</text><text font-family='Segoe UI Semibold' x='90' y='27' text-anchor='start'>${entity.esg.factors.e}</text><rect fill='rgb(241, 242, 242)' x='110' y='21' width='100px' height='3px' rx='2px' ry='2px' stroke-linejoin='round' /><rect fill='rgb(152, 154, 156)' x='110' y='21' width='${entity.esg.factors.e}px' height='3px' rx='2px' ry='2px' stroke-linejoin='round' /><text x='0' y='45' text-anchor='start'>Social</text><text font-family='Segoe UI Semibold' x='90' y='45' text-anchor='start'>${entity.esg.factors.s}</text><rect fill='rgb(241, 242, 242)' x='110' y='39' width='100px' height='3px' rx='2px' ry='2px' stroke-linejoin='round' /><rect fill='rgb(152, 154, 156)' x='110' y='39' width='${entity.esg.factors.s}px' height='3px' rx='2px' ry='2px' stroke-linejoin='round' /><text x='0' y='63' text-anchor='start'>Governance</text><text font-family='Segoe UI Semibold' x='90' y='63' text-anchor='start'>${entity.esg.factors.g}</text><rect fill='rgb(241, 242, 242)' x='110' y='57' width='100px' height='3px' rx='2px' ry='2px' stroke-linejoin='round' /><rect fill='rgb(152, 154, 156)' x='110' y='57' width='${entity.esg.factors.g}px' height='3px' rx='2px' ry='2px' stroke-linejoin='round' /></g></svg>",
                                            "width": "200px",
                                            "height": "100px",
                                            "horizontalAlignment": "Left"
                                        }
                                    ],
                                    "width": "stretch"
                                }
                            ]
                        }
                    ]
                }
            ],
            "horizontalAlignment": "Center",
            "spacing": "Padding",
            "style": "default"
        },
        {
            "type": "Container",
            "minHeight": "10px",
            "items": []
        },
        {
            "type": "Container",
            "minHeight": "10px",
            "$when": "${!empty(entity.companyNewsSummary.summary)}",
            "items": [
              {
                  "type": "TextBlock",
                  "fontType": "Default",
                  "weight": "bolder",
                  "text": "Company Information",
                  "wrap": true,
                  "maxLines": 2
                },
                {
                    "type": "TextBlock",
                    "text": "${if(empty([entity.companyNewsSummary.summary]), '', entity.companyNewsSummary.summary)}\n",
                    "wrap": true,
                    "maxLines": 6
                }
            ]
          },
        {
            "type": "Container",
            "minHeight": "10px",
            "items": []
        },
        {
            "type": "Container",
            "minHeight": "10px",
            "$when": "${!empty(entity.prompts)}",
            "items": [
                {
                    "type": "TextBlock",
                    "text": "Suggested Prompts",
                    "weight": "Bolder"
                },
                {
                    "type": "Container",
                    "$data": "${entity.prompts}",
                    "verticalContentAlignment": "Center",
                    "items": [
                        {
                            "type": "ColumnSet",
                            "columns": [
                                {
                                    "type": "Column",
                                    "width": "21px",
                                    "items": [
                                        {
                                            "type": "Image",
                                            "url": "data: image/png;base64,",
                                            "width": "15px",
                                            "height": "15px",
                                            "horizontalAlignment": "Left"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "auto",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "${$data}",
                                            "horizontalAlignment": "Left",
                                            "wrap": true,
                                            "maxLines": 3
                                        }
                                    ],
                                    "selectAction": {
                                        "type": "Action.Execute",
                                        "title": "${$data}",
                                        "verb": "suggestedPrompt",
                                        "data": "${$data}"
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "Container",
            "minHeight": "10px",
            "items": []
        },
        {
            "type": "Container",
            "minHeight": "10px",
            "$when": "${!empty(entity.otherCompanies)}",
            "items": [
                {
                    "type": "TextBlock",
                    "text": "Similar Companies",
                    "weight": "Bolder"
                },
                {
                    "type": "Container",
                    "$data": "${entity.otherCompanies}",
                    "verticalContentAlignment": "Center",
                    "items": [
                        {
                            "type": "ColumnSet",
                            "columns": [
                                {
                                    "type": "Column",
                                    "width": "21px",
                                    "items": [
                                        {
                                            "type": "Image",
                                            "url": "data:image/png;base64,",
                                            "width": "15px",
                                            "height": "15px",
                                            "horizontalAlignment": "Left"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "auto",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "${$data.name}",
                                            "horizontalAlignment": "Left",
                                            "wrap": true,
                                            "maxLines": 3
                                        }
                                    ],
                                    "selectAction": {
                                        "type": "Action.Execute",
                                        "title": "${$data.name}",
                                        "verb": "otherCompany",
                                        "data": "${$data.name}"
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
   ],
    "msteams": {
        "width": "Full"
    }
}